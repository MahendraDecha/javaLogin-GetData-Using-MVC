<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>firstMvc</display-name>
  
  <servlet>
    <description></description>
    <display-name>controlleServlet</display-name>
    <servlet-name>controlleServlet</servlet-name>
    <servlet-class>com.org.mvc.controlleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>controlleServlet</servlet-name>
    <url-pattern>/controlleServlet</url-pattern>
  </servlet-mapping>
   
   <jsp-config>
  	<taglib>
    	<taglib-uri>http://java.sun.com/firstMvc/core-rt</taglib-uri>
    	<taglib-location>/WEB-INF/tld/c-rt.tld</taglib-location>
  	</taglib>
  </jsp-config>
  
</web-app>